nFreq = 36;
nLocs = 5;
simLen = 76000;
fs = 44100;

%I2input = genI2input(nFreq,nLocs,simLen,fs);

I2input = loadI2input();

netcon=eye(N_pre,N_post)

IappI2 = 0.1
E_exc = 0
tauR = 1
tauD = 3

% iI2(X,t) = I2input(ceil((t+dt*0.01)/dt),:);

iI2(X,t) = IappI2 .* I2input(ceil((t+dt*0.01)/dt),:) * netcon .* (X - E_exc);


monitor iI2

@isyn += iI2(X,t)
